{
  "name": "ui-agent-benchmark",
  "version": "0.1.0",
  "description": "UI Testing Agent Benchmark Framework - 批量评测 UI Testing Agent 的标准化实验框架",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "uibench": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint && pnpm format:check && pnpm typecheck"
  },
  "keywords": [
    "ui-testing",
    "agent",
    "benchmark",
    "evaluation"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@midscene/web": "^1.0.4",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "execa": "^9.5.2",
    "get-port": "^7.1.0",
    "p-limit": "^6.1.0",
    "pino": "^9.6.0",
    "puppeteer": "^24.34.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
